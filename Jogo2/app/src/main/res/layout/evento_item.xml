<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="75dp"
        android:layout_height="75dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.03"
        app:srcCompat="@drawable/usuario" />

    <TextView
        android:id="@+id/nombreEE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_toEndOf="@+id/imageView7"
        android:text="TextView"
        android:textColor="#5E5E5E"
        android:textSize="30dp" />

    <TextView
        android:id="@+id/ubicacionEE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/nombreEE"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:layout_toEndOf="@+id/imageView7"
        android:text="TextView"
        android:textColor="#8D8D8D" />

    <TextView
        android:id="@+id/descripcionEE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ubicacionEE"
        android:layout_marginStart="30dp"
        android:layout_marginTop="10dp"
        android:layout_toEndOf="@+id/imageView7"
        android:text="TextView"
        android:textColor="#595454" />

    <TextView
        android:id="@+id/diaEE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/nombreEE"
        android:layout_marginStart="247dp"
        android:layout_marginTop="-24dp"
        android:layout_toEndOf="@+id/imageView7"
        android:text="TextView" />

    <TextView
        android:id="@+id/horaEE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/nombreEE"
        android:layout_marginStart="247dp"
        android:layout_marginTop="10dp"
        android:layout_toEndOf="@+id/imageView7"
        android:text="TextView" />
</RelativeLayout>